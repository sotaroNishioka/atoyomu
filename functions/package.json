{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "lsof -t -i:9999 | xargs kill -9 && lsof -t -i:8888 | xargs kill -9 && lsof -t -i:5001 | xargs kill -9 && lsof -t -i:4000 | xargs kill -9 && tsc-watch --onFirstSuccess 'firebase emulators:start'",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.11",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^3.21.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.5.4"
  },
  "private": true
}
